    {
      "controlChangeNumber": 19,
      "name": "Tone B",
      "valueRange": { "min": 0, "max": 127 }
    },

    {
      "controlChangeNumber": 21,
      "name": "Channel A Effect Select",
      "additionalInfo": "1 = Boost, 2 = Drive, 3 = Fuzz",
      "valueRange": {
        "min": 1,
        "max": 3,
        "discreteValues": [
          { "value": 1, "name": "Boost" },
          { "value": 2, "name": "Drive" },
          { "value": 3, "name": "Fuzz" }
        ]
      }
    },
    {
      "controlChangeNumber": 22,
      "name": "PARA / A >> B / B >> A Select",
      "additionalInfo": "1 = PARA, 2 = A >> B, 3 = B >> A",
      "valueRange": {
        "min": 1,
        "max": 3,
        "discreteValues": [
          { "value": 1, "name": "PARA" },
          { "value": 2, "name": "A >> B" },
          { "value": 3, "name": "B >> A" }
        ]
      }
    },
    {
      "controlChangeNumber": 23,
      "name": "Channel B Effect Select",
      "additionalInfo": "1 = Fuzz, 2 = Drive, 3 = Boost",
      "valueRange": {
        "min": 1,
        "max": 3,
        "discreteValues": [
          { "value": 1, "name": "Fuzz" },
          { "value": 2, "name": "Drive" },
          { "value": 3, "name": "Boost" }
        ]
      }
    },

    {
      "controlChangeNumber": 100,
      "name": "Expression",
      "additionalInfo": "0 = heel, 127 = toe (expression target set by dips)",
      "valueRange": { "min": 0, "max": 127 }
