      "name": "Left Channel Mode",
      "additionalInfo": "0–1 = Mode 1, 2 = Mode 2, 3–127 = Mode 3",
      "valueRange": {
        "min": 0,
        "max": 127,
        "discreteValues": [
          { "value": 0, "name": "Mode 1 (Micro Loop)" },
          { "value": 2, "name": "Mode 2 (Envelope)" },
          { "value": 3, "name": "Mode 3 (Glide ≥ 3)" }
        ]
      }
    },

    {
      "controlChangeNumber": 22,
      "name": "Routing",
      "additionalInfo": "0–1 = L→R, 2 = L↔R, 3–127 = R→L",
