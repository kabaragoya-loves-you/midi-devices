          { "value": 0, "name": "Thirty-second" },
          { "value": 1, "name": "Sixteenth" },
          { "value": 2, "name": "Triplet Sixteenth" },
          { "value": 3, "name": "Eighth" },
          { "value": 4, "name": "Triplet Eighth" },
          { "value": 5, "name": "Dotted Eighth" },
          { "value": 6, "name": "Quarter" },
          { "value": 7, "name": "Triplet Quarter" },
          { "value": 8, "name": "Dotted Quarter" },
          { "value": 9, "name": "Half" },
          { "value": 10, "name": "Dotted Half" },
          { "value": 11, "name": "Whole/Double Whole" }
        ]
      }
