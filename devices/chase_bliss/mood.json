          { "value": 3, "name": "Right → Left" }
        ]
      }
    },

    {
      "controlChangeNumber": 23,
      "name": "Right Channel Mode",
      "additionalInfo": "0–1 = Mode 4, 2 = Mode 5, 3–127 = Mode 6",
      "valueRange": {
        "min": 0,
        "max": 127,
        "discreteValues": [
          { "value": 0, "name": "Mode 4 (Slip)" },
          { "value": 2, "name": "Mode 5 (Stretch)" },
          { "value": 3, "name": "Mode 6 (Envelope ≥ 3)" }
        ]
      }
    },

    {
      "controlChangeNumber": 24,
      "name": "Clock Division",
