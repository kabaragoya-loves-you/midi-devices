        "min": 0,
        "max": 127,
        "discreteValues": [
          { "value": 0, "name": "Bypass (both channels off)" },
          { "value": 127, "name": "Engage last saved channels" }
        ]
      }
    },
    {
      "controlChangeNumber": 103,
      "name": "Bypass Switch (Explicit Channel Routing)",
      "additionalInfo": "0 = bypass both; 45 = B only; 85 = A only; 127 = A+B",
      "valueRange": {
        "min": 0,
        "max": 127,
        "discreteValues": [
          { "value": 0, "name": "Bypass (both channels off)" },
          { "value": 45, "name": "Channel B on, Channel A bypassed" },
          { "value": 85, "name": "Channel A on, Channel B bypassed" },
          { "value": 127, "name": "Both channels on" }
        ]
      }
    }
